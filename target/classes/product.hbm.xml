<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.maybank.pms.model.Product" table="product100">

		<id name="productId" type="int" column="EID">
			<generator class="assigned" />
		</id>

		<property name="productName" column="ProdName" />
		<property name="quantityOnHand" column="QOH" />
		<property name="price" column="Price" />


	</class>
</hibernate-mapping>